#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/iojs" ]; then
  exec "$basedir/iojs"  "$basedir/../node-readability/src/cli.js" "$@"
else 
  exec iojs  "$basedir/../node-readability/src/cli.js" "$@"
fi
