<html>
  <head>
    <meta name="referrer" content="no-referrer" />
    <link rel="stylesheet" href="/css/bootstrap.css" />
    <link rel="icon" href="../public/favicon.ico" />
    <title>SimpliReader</title>
    <style type="text/css">
      * {
        box-sizing: border-box;
        margin: 0;
        padding: 0;
      }

      body {
        font-family: "Microsoft-yahei", sans-serif;
        font-size: 18px;
        margin: 30px;
      }

      ul {
        list-style: none;
      }

      a {
        text-decoration: none;
      }

      .btn {
        border-radius: 5px;
        cursor: pointer;
        padding: 5px 15px;
        font-size: 15px;
      }

      .article-unit {
        padding: 10px;
        margin-bottom: 15px;
        background-color: #e0ecee;
        border-radius: 10px;
        overflow-wrap: break-word;
      }

      .back {
        margin-top: 20px;
        width: 100%;
      }

      .back #back{
        width: 100%;
      }

    </style>
  </head>
</html>
